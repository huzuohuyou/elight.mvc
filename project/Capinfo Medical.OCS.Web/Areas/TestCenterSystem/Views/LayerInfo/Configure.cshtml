    {{#  layui.each(d.list, function(index, item){ }}
    <tr>
        <td>
            <input type="checkbox" lay-skin="primary" value="{{item.ID}}">
        </td>
        <td>{{item.SHOW_CODE}}</td>
        <td>{{item.DATAITEM_CODE}}</td>
        <td>{{item.DATAITEM_NAME}}</td>
        <td>{{item.DATA_TYPE}}</td>
        <td>{{item.VALUE_COUNT}}</td>
    </tr>
    {{#  }); }}
    {{# if(d.list.length<=0) { }}
    <tr style="color: red">
        <td colspan="6">查无数据。</td>
    </tr>
    {{# } }}
</script>

