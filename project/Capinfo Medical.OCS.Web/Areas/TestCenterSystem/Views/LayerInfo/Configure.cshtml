    {{# } }}
</script>

<script>

    $("#process_content").hide();
    $("#log_content").hide();
    $("#test_content").hide();
    var layerId = $.getQueryString("primaryKey");
    function initSet() {
        if (layerId) {
            $.ajax({
                url: "/TestCenterSystem/LayerInfo/GetForm",
                data: { primaryKey: layerId },
                type: "post",
                dataType: "json",
                async: false,
                success: function (data) {
                    $("#form").formSerialize(data);
                }
            });
        }

    }

    function initProcLog() {
        var primaryKey = $.getQueryString("primaryKey");
        if (primaryKey) {
            $.ajax({
                url: "/TestCenterSystem/KPI/ProcLog",
                data: { primaryKey: primaryKey },
                type: "get",
                dataType: "json",
                async: false,
                success: function (data) {
                    $("#startDate").val(data.startDate);
                    $("#endDate").val(data.endDate);
                    $("#patientNo").text(data.patientId);
                }
            });
        }

    }

    //根据指定url刷新页面
    function refrenshParent() {
        for (var i = 0; i < parent.length; i++) {
            if (parent[i].document.URL.indexOf("/TestCenterSystem/LayerInfo/Index") > 0) {
                parent[i].initGrid();
                break;
            }
        }
    }
    //TC_SD_EKPI_FORMULA公式信息显示

    function btn_validate() {
        $.layerConfirm({
            content: "校验当前KPI？",
            callback: function () {
                $.ajax({
                    type: "POST",
                    url: "/TestCenterSystem/LayerInfo/Validate",
                    dataType: "json",
                    data: { SD_LAYER_ID: $("#SD_LAYER_ID").val(), SD_LAYER_FORMULA: $("#SD_LAYER_FORMULA").val() },
                    success: function (data) {
                        $.layerMsg(data.message, data.state);
                    },
                });
            }
        });
    }

    layui.use('form', function () {
        var form = layui.form();
        initSet();
        showHideCode()
        //initProcLog();
        form.render();
        form.on('submit(add)', function (data) {
            //btn_validate();
            $.formSubmit({
                url: "/TestCenterSystem/LayerInfo/UpdateFormula",
                data: data.field
            });
            return false;
        });
    });

</script>

<script type="text/javascript">

    function searchDataItem(keywords) {
        $.ajax({
            type: "GET",
            url: "/TestCenterSystem/Formula/GetListTree",
            data: { projectId: 1, sdId: 1, kpiid: $("#KPI_ID").val(), dataitemid: treeNode.id },
            dataType: "json",
            success: function (data) {
                initGrid();
            },
            error: function (data) {
                $.layerMsg(data.msg, "warning");
                initGrid();
            }
        });
    }

    var paging;
    layui.config({
        base: parent._baseUrl
    }).use('paging', function () {

        paging = layui.paging();
        var setting = {
            data: {
                simpleData: {
                    enable: true
                }
            },
            check: {
                enable: false,
                chkStyle: "checkbox",
                chkDisabledInherit: false
            },
            async: {
                enable: true,
                url: "/TestCenterSystem/Formula/GetListTree?keywords=" + $("#keyWord").val(),
                dataType: "text",
                autoParam: ["id", "name"]
            }, callback: {
                onAsyncSuccess: function (event, treeId, treeNode, msg) {
                    var node = zTreeObj.getNodesByFilter(function (node) {
                        return node.level == 1
                    }, true);
                    zTreeObj.selectNode(node);
                    zTreeObj.setting.callback.onClick(event, zTreeObj.setting.treeId, node);
                },
                onDblClick: function (event, treeId, treeNode) {
                    addParam(treeNode);
                },
                onClick: function (event, treeId, treeNode) { }
            }
        };

        var zTreeObj = $.fn.zTree.init($("#treeItem"), setting);
        initGrid();
    });

    function initTree() {
        layui.config({
            base: parent._baseUrl
        }).use('paging', function () {

            paging = layui.paging();
            var setting = {
                data: {
                    simpleData: {
                        enable: true
                    }
                },
                check: {
                    enable: false,
                    chkStyle: "checkbox",
                    chkDisabledInherit: false
                },
                async: {
                    enable: true,
                    url: "/TestCenterSystem/Formula/GetListTree?keywords=" + $("#keyWord").val(),
                    dataType: "text",
                    autoParam: ["id", "name"]
                }, callback: {
                    onAsyncSuccess: function (event, treeId, treeNode, msg) {
                        var node = zTreeObj.getNodesByFilter(function (node) {
                            return node.level == 1
                        }, true);
                        zTreeObj.selectNode(node);
                        zTreeObj.setting.callback.onClick(event, zTreeObj.setting.treeId, node);
                    },
                    onDblClick: function (event, treeId, treeNode) {
                        addParam(treeNode);
                    },
                    onClick: function (event, treeId, treeNode) { }
                }
            };

            var zTreeObj = $.fn.zTree.init($("#treeItem"), setting);

        });

    }

    function initBtnStatus() {
        return;
        $.ajax({
            type: "GET",
            url: "/TestCenterSystem/KPI/BtnStatus",
            data: { kpiId: $("#KPI_ID").val() },
            dataType: "json",
            success: function (data) {
                if (data.PROC_STAT_CODE == '4') {

                    $("#exe").text("已执行");
                    //$("#excute").get(0).className = "layui-btn layui-btn-disabled layui-btn-small";
                    //$("#excute").attr("disabled", "disabled");

                    $("#lock").text("已锁定");
                    //$("#lock").get(0).className = "layui-btn layui-btn-disabled layui-btn-small";
                    //$('#lock').attr("disabled", "disabled");

                } else if (data.PROC_STAT_CODE == '2') {
                    $("#exe").text("已执行");
                    //$("#excute").get(0).className = "layui-btn layui-btn-disabled layui-btn-small";
                    //$('#excute').attr("disabled", "disabled");
                } else {
                    $("#exe").text("执行");
                    //$("#excute").get(0).className = "layui-btn layui-btn-normal layui-btn-small";
                    //$('#excute').attr("disabled", "disabled");

                    $("#lock").text("锁定");
                    //$("#lock").get(0).className = "layui-btn layui-btn-normal layui-btn-small";
                    //$('#lock').attr("disabled", "disabled");
                }
            },
            error: function (data) {
                $.layerMsg(data.msg, "warning");
            }
        });
    }

    function initGrid() {
        paging.init({
            url: '/TestCenterSystem/LayerParam/GetList',
            elem: '#content',
            tempElem: '#contentTpl',
            params: {
                layerId: $.getQueryString("primaryKey"),
            },
            pageConfig: {
                elem: '#paged',
                pageSize: 10,
            },
            success: function () {

            },
            fail: function (msg) {
                top.layer.msg(msg);
            }
        });
    }

    function addParam(treeNode) {
        $.ajax({
            type: "GET",
            url: "/TestCenterSystem/LayerParam/Add",
            data: { layerId: $("#SD_LAYER_ID").val(), dataitemid: parseInt(treeNode.id) },
            dataType: "json",
            success: function (data) {
                initGrid();
            },
            error: function (data) {
                $.layerMsg(data.msg, "warning");
                initGrid();
            }
        });
    }

    function btn_delete() {
        var pids = $("#paramgridList").gridSelectedRowValue();
        if (pids.length < 1) {
            $.layerMsg("请勾选需要删除的选项。", "warning");
            return;
        }

        $.layerConfirm({
            content: "您已选中" + pids.length + "条数据, 确定删除吗？",
            callback: function () {
                $.ajax({
                    type: "GET",
                    url: '/TestCenterSystem/LayerParam/Delete?primaryKey=' + pids[0],
                    dataType: "json",
                    success: function (data) {
                        initGrid();
                        $.layerMsg("删除成功！", 1);
                    },
                    error: function (data) {
                        $.layerMsg("删除失败！", 1);
                        initGrid();
                    }
                });
            }
        });
    }

</script>

<script type="text/javascript">
    //tab页切换
    var paging;
    layui.config({
        base: parent._baseUrl
    }).use(['paging', 'form', 'layer', 'element', 'laydate'], function () {
        var layer = parent.layer || layui.layer;
        var form = layui.form();
        var element = layui.element();
        var laydate = layui.laydate;
        paging = layui.paging();
        initBtnStatus();
        //给input赋值
        $('#startDate').val(laydate.now(-30, 'YYYY-MM-DD'));
        $('#endDate').val(laydate.now(0, 'YYYY-MM-DD'));
        //initProcLog();

    });
    var firsttime = 0;
    var note = "";

    function guid() {
        function S4() {
            return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
        }
        return (S4() + S4() + "-" + S4() + "-" + S4() + "-" + S4() + "-" + S4() + S4() + S4());
    }

    function showHideCode() {
        console.log("firsttime:" + firsttime)
        //$("#showdiv").toggle();
        if (firsttime > 0 && firsttime % 2 == 0) {
            $("#show_more").text('收起');
            $("#SD_EKPI_ALGO").text("逻辑描述：" + note);
        } else {
            $("#show_more").text('展开');
            $("#SD_EKPI_ALGO").text("逻辑描述：" + note.substring(0, 60));
        }
        if (firsttime == 0) {
            $.ajax({
                type: "POST",
                url: '/TestCenterSystem/LayerInfo/GetForm?primaryKey=' + $("#SD_LAYER_ID").val(),
                dataType: "json",
                success: function (data) {
                    note = data.SD_LAYER_DESC;
                    $("#SD_EKPI_ALGO").text("逻辑描述：" + note.substring(0, 60));
                    firsttime = firsttime + 1;
                },
                error: function (data) {
                }
            });
        }
        firsttime = firsttime + 1;
    }

</script>

<script>
    //测试页面
    function btn_test(btn_flag) {
        if ($("#startDate").val() > $("#endDate").val()) {
            top.layer.msg("开始时间大于结束时间！");
            return;
        }
        $("#process_content").hide();
        $("#log_content").hide();
        $("#test_content").show();
        paging.init({
            url: '/TestCenterSystem/LayerInfo/Test',
            elem: '#kpi-result-content',
            tempElem: '#kpi-result-contentTpl',

            params: {
                SD_LAYER_ID: parseInt($("#SD_LAYER_ID").val()),
                START_DATE: $("#startDate").val(),
                END_DATE: $("#endDate").val(),
                PATIENT_NO: $("#patientNo").val(),
                FLAG: btn_flag
            },
            pageConfig: {
                elem: '#paged',
                pageSize: 100,
            },
            success: function () {
                if (btn_flag == "excute") {
                    top.layer.msg("执行成功！！！");
                } else if (btn_flag == "lock") {
                    top.layer.msg("锁定成功！！！");
                }
                initBtnStatus();
            },
            fail: function (msg) {
                top.layer.msg(msg);
                initBtnStatus();
            }
        });
    }

    function btn_lock(btn_flag) {
        if (btn_flag == "lock" && $('#lock').hasClass('layui-btn-disabled')) {
            return;
        }
        $.ajax({
            type: "GET",  //提交方式
            url: '/TestCenterSystem/LayerInfo/Lock',
            data: {
                SD_LAYER_ID: parseInt($("#SD_LAYER_ID").val()),
                START_DATE: $("#startDate").val(),
                END_DATE: $("#endDate").val(),
                PATIENT_NO: $("#patientNo").val(),
                FLAG: btn_flag
            },

            success: function (data) {
                var obj = JSON.parse(data);
                top.layer.msg(obj.message)
                initBtnStatus();
                refrenshParent();
            },
            fail: function (data) {
                var obj = JSON.parse(data);
                top.layer.msg(obj.message);
                initBtnStatus();
            }
        });
    }

    function btn_excute(btn_flag) {
        if ($("#startDate").val() > $("#endDate").val()) {
            top.layer.msg("开始时间大于结束时间！");
            return;
        }
        //$("#process_content").show();
        $("#test_content").hide();
        //$("#log_content").show();

        //alert($.cookie("kpi_client_key"));
        //if (btn_flag == "excute" && $('#excute').hasClass('layui-btn-disabled')) {
        //    return;
        //}
        $.ajax({
            type: "GET",  //提交方式
            url: '/TestCenterSystem/LayerInfo/Excute',
            data: {
                SD_LAYER_ID: parseInt($("#SD_LAYER_ID").val()),
                START_DATE: $("#startDate").val(),
                END_DATE: $("#endDate").val(),
                PATIENT_NO: $("#patientNo").val(),
                FLAG: btn_flag,
                CLIENT_KEY: $.cookie("kpi_client_key"),
            },
            success: function (data) {
                var obj = JSON.parse(data);
                top.layer.msg(obj.message)
                initBtnStatus();
                refrenshParent();
            },
            fail: function (data) {
                var obj = JSON.parse(data);
                top.layer.msg(obj.message);
                initBtnStatus();
            }
        });
    }

    function btn_clear() {
        $.ajax({
            type: "GET",  //提交方式
            url: '/TestCenterSystem/LayerInfo/Clear',
            data: { primaryKey: parseInt($("#SD_LAYER_ID").val()) },
            success: function (data) {
                var obj = JSON.parse(data);
                top.layer.msg(obj.message)
                initBtnStatus();
                refrenshParent();
            },
            fail: function (msg) {
                top.layer.msg(msg);
                initBtnStatus();
            }
        });
    }

    //执行结果导出
    function btn_export() {
        $.ajax({
            type: "GET",  //提交方式
            url: "/TestCenterSystem/LayerInfo/ExportResult",
            data: { primaryKey: parseInt($("#SD_LAYER_ID").val()) },
            success: function (obj) {
                obj = JSON.parse(obj);
                if (obj.title === null)
                    $.layerMsg("层级管理执行结果导出失败！", 2);
                else if (obj.data.length == 0) {
                    $.layerMsg("没有数据可导！", 2);
                }
                else {
                    $.JSONToExcelConvertor(obj.data, "层级管理_" + layerId + "_执行结果表", obj.title);
                }
            }
        });
    }

</script>

<script>
    @*<script src="~/Scripts/jquery-1.6.4.min.js"></script>*@
    //长连接客户端生成唯一Key
    initKey();
    function initKey() {
        if ($.cookie('kpi_client_key') == null) {
            var kpi_client_key = guid();
            $.cookie('kpi_client_key', kpi_client_key, { expires: 999, path: '/' });
        }
        console.log('kpi_client_key:' + $.cookie('kpi_client_key'))
    }
    //layui进度条
    layui.use('element', function () {
        var $ = layui.jquery
        , element = layui.element(); //Tab的切换功能，切换事件监听等，需要依赖element模块
        // 触发事件进度状态长连接
        $(function () {
            var chat = $.connection.processHub;
            chat.client.addNewMessageToPage = function (name, percent, title, note) {
                var kpi_client_key = $.cookie("kpi_client_key")
                console.log(percent);
                if (name == kpi_client_key) {
                    $("#process_content").show();
                    $("#test_content").hide();
                    $("#log_content").show();
                    $("#exe").text("执行中");
                    element.progress('kpi_progress', percent)
                    var str = $('#process_note').val(); //先获取原有的值
                    $('#process_note').val(note + "\n" + str); //拼接新值
                    if (percent == "100%") {
                        $("#exe").text("已执行");
                    }
                }
            };
            $.connection.hub.start().done(function () {
            });
        });
    });
</script>