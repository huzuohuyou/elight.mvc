<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- 软件名称 -->
  <add key="SoftwareName" value="Elight.MVC" />
  <!-- 用户登录信息保存模式：Session、Cookie -->
  <add key="LoginProvider" value="Cookie" />
  <!-- 用户登录信息保存时间（单位：分钟）-->
  <add key="LoginTimeout" value="120" />
  <!-- 主键自增 MySql设置false SqlServer true（默认true）-->
  <add key="AutoIncrement" value="false" />
</appSettings>